{
  "name": "smart-locale-redirect/smart-locale-redirect",
  "description": "A Shopware 6 plugin to redirect based on browser locale",
  "type": "shopware-platform-plugin",
  "license": "MIT",
  "require": {
    "shopware/core": "~6.3"
  },
  "autoload": {
    "psr-4": {
      "SmartLocaleRedirect\\": "src/"
    }
  },
  "extra": {
    "shopware-plugin-class": "SmartLocaleRedirect\\SmartLocaleRedirect",
    "label": {
      "en-GB": "Smart Locale Redirect",
      "de-DE": "Smart Locale Redirect"
    }
  }
}
